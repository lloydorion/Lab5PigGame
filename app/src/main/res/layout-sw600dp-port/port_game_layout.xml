<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="horizontal"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:baselineAligned="false">

    <fragment
        android:id="@+id/second_fragment"
        android:name="edu.lanecc.lloydo.lab5_piggame.GameScreenFragment"
        android:layout_width="619dp"
        android:layout_height="657dp"
        android:layout_weight="1"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/first_fragment"
        tools:layout="@layout/game_screen_fragment" />

    <fragment
        android:id="@+id/first_fragment"
        android:name="edu.lanecc.lloydo.lab5_piggame.TitleScreenFragment"
        android:layout_width="609dp"
        android:layout_height="569dp"
        android:layout_weight="1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout="@layout/title_screen_fragment"
        tools:layout_editor_absoluteY="-275dp" />

</androidx.constraintlayout.widget.ConstraintLayout>