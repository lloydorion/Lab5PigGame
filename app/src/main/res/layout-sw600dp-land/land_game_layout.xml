<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="horizontal"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:baselineAligned="false">

    <fragment
        android:id="@+id/first_fragment"
        android:name="edu.lanecc.lloydo.lab5_piggame.TitleScreenFragment"
        android:layout_width="759dp"
        android:layout_height="617dp"
        android:layout_marginStart="350dp"
        android:layout_weight="1"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="@+id/second_fragment"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        tools:layout="@layout/title_screen_fragment" />

    <fragment
        android:id="@+id/second_fragment"
        android:name="edu.lanecc.lloydo.lab5_piggame.GameScreenFragment"
        android:layout_width="667dp"
        android:layout_height="666dp"
        android:layout_marginEnd="360dp"
        android:layout_weight="1"
        app:layout_constraintEnd_toEndOf="parent"
        tools:layout="@layout/game_screen_fragment"
        tools:layout_editor_absoluteY="0dp" />

</androidx.constraintlayout.widget.ConstraintLayout>